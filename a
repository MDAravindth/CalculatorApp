def b = "master"
pipeline{
    agent any
    stages{
        stage("SCM checkout"){
            steps{
                git url: 'https://github.com/MDAravindth/CalculatorApp.git',
                   branch: "${b}"
                }
        }
        stage("build"){
            steps{
                bat 'mvn clean package'
            }
        }
    }
}
